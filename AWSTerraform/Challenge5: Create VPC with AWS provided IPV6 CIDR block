Create VPC with AWS provided IPV6 CIDR block

resource "aws_vpc" "datacenter-vpc" {
    cidr_block = "10.0.0.0/16"
    assign_generated_ipv6_cidr_block = true
    tags = {
        Name = "datacenter-vpc"
    }
}

provider.tf
terraform {
  required_providers {
    aws = {
      source = "hashicorp/aws"
      version = "5.91.0"
    }
  }
}

provider "aws" {
  region  = "us-east-1"
}
